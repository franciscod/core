r0 bh     black hole, always 00
r1 a      general purpose
r2 b      general purpose
r3 c      general purpose
r4 d      general purpose
r5 e      general purpose
r6 f      general purpose
r7 g      general purpose

                    76543210
r8 fl         flags Q___CNVZ
r9 ip rip pc  instruction pointer
ra sp         stack pointer
rb ??         ????
rc cs         ????
rd ds         data segment (8 bit, shift 16)
re lr         link register
rf wh         white hole, always ff
        

call addr
    mov  lr addr
    xchg lr ip

ret
    mov  ip lr

ip un valor
registro un valor de ip

                                           AKA
store [imm10] r4    11 AA AAAA AAAA RRRR   writeToMemImmediate
load  r4 [imm10]    10 AA AAAA AAAA RRRR   readFromMemImmediate
                                         
asr   r4 imm4       01 11 C111 IIII RRRR 
lsl   r4 imm4       01 11 C110 IIII RRRR 
lsr   r4 imm4       01 11 C101 IIII RRRR 
xor   r4 r4         01 11 C100 RRRR RRRR  not (white)
and   r4 r4         01 11 C011 RRRR RRRR 
or    r4 r4         01 11 C010 RRRR RRRR 
sub   r4 r4         01 11 C001 RRRR RRRR  Compare
add   r4 r4         01 11 C000 RRRR RRRR 
                                         
mov   r4 imm8       01 10 DDDD DDDD RRRR   SetRegister
setcc imm8          01 01 C___ MMMM QQQQ   
                                         
store [r4] r4       00 11 C___ RRRR RRRR   writeToMemIndirect
load  r4 [r4]       00 10 C___ RRRR RRRR   readFromMemIndirect
xchg  r4 r4         00 01 C___ RRRR RRRR   call
